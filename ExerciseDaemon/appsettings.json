{
  "Serilog": {
    "Using": [
      "AWS.Logger.SeriLog"
    ],
    "LogGroup": "ecs-exercise-daemon",
    "Region": "us-east-1",
    "MinimumLevel": { "Default": "Information" },
    "Override": {
      "Microsoft": "None",
      "System": "Error",
      "Microsoft.AspNetCore.Mvc.Internal.ControllerActionInvoker": "None"
    },
    "WriteTo": [
      { "Name": "AWSSeriLog" },
      {
        "Name": "Console",
        "Args": { "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact" }
      },
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Strava": {
    "ClientId": 0,
    "ClientSecret": ""
  },
  "DynamoDb": {
    "RegionEndpoint": "us-east-1",
    "ServiceUrl": ""
  },
  "Slack": {
    "SlackWebhookUrl": "",
    "SlackWorkspaceUrl": "",
    "SlackChannelId": "",
    "SlackBotUserAccessToken": "",
    "SlackClientId": "",
    "SlackClientSecret": "",
    "SlackRedirectUri": ""
  },
  "Google": {
    "ApiKey": ""
  }
}